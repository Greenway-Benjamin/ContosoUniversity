@model IEnumerable<ContosoUniversity.Models.SchoolViewModels.EnrollmentDateGroup>

<h2>Student Body</h2>
<h4>Look how much we have grown over the years!</h4>

<table>
    <tr>
        <th>
            Enrollment Date
        </th>
        <th>
            Students
        </th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.EnrollmentDate)
            </td>
            <td>
                @item.StudentCount
            </td>
        </tr>
    }
</table>
